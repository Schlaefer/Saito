{
  "name": "schlaefer/saito",
  "description": "Saito - The Threaded Forum",
  "keywords": ["forum", "cakephp"],
  "license": "MIT",
  "homepage": "http://saito.siezi.com/",
  "support": {
    "issues": "https://github.com/Schlaefer/Saito/issues",
    "source": "https://github.com/Schlaefer/Saito",
    "support": {
      "forum": "http://saito.siezi.com/forum/"
    }
  },
  "require": {
    "php": ">=5.4.16",
    "cakephp/cakephp": "3.0.*-dev",
    "mobiledetect/mobiledetectlib": "2.*",
    "cakephp/debug_kit": "3.0.*-dev",
    "markstory/geshi": "3.0.*-dev",
    "kzykhys/ciconia": "~1.0.3",
    "jbbcode/jbbcode": "~1.3",
    "aura/di": "~2.1"
  },
  "require-dev": {
    "d11wtq/boris": "1.0.*",
    "phpmd/phpmd": "@stable",
    "phpunit/phpunit": "*"
  },
  "suggest": {
    "cakephp/cakephp-codesniffer": "Allows to check the code against the coding standards used in CakePHP."
  },
  "autoload": {
    "psr-4": {
      "App\\": "src",
      "Plugin\\": "plugins/",
      "Saito\\": "src/Lib/Saito/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests",
      "Cake\\Test\\Fixture\\": "./vendor/cakephp/cakephp/tests/Fixture"
    }
  },
  "scripts": {
    "post-install-cmd": "App\\Console\\Installer::postInstall"
  },
  "minimum-stability" : "dev",
  "prefer-stable": true
}
