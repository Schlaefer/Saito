parameters:
    level: 1
    paths:
        - plugins/
        - src/
    excludes_analyse:
        - **/*.ctp
        - **/TestCase/*
    autoload_files:
        - tests/bootstrap.php
    ignoreErrors:
        - '#Access to an undefined property object::\$sub#'
        - '#Call to an undefined method.*Table::findBy.*\(\)#'
