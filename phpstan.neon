parameters:
    level: 3
    paths:
        - plugins/
        - src/
    excludes_analyse:
        - **/*.ctp
        - **/TestCase/*
        # Inline classes are not picked up by phpstan
        - */BbcodeParser/src/Lib/jBBCode/Definitions/*
    autoload_files:
        - tests/bootstrap.php
    ignoreErrors:
        - '#Call to an undefined method.*Table::findBy.*\(\)#'
