; This file is for unifying the coding style for different editors and IDEs.
; More information at http://editorconfig.org

root = true

[*]
indent_style = space
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_size = 2
charset = utf-8

[*.bat]
end_of_line = crlf

[*.ctp]
indent_style = tab;

[*.php]
indent_style = tab;

