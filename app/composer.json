{
  "name": "schlaefer/saito",
  "description": "Saito - The Threaded Forum",
  "keywords": ["forum", "cakephp"],
  "license": "MIT",
  "homepage": "http://saito.siezi.com/",
  "support": {
    "issues": "https://github.com/Schlaefer/Saito/issues",
    "source": "https://github.com/Schlaefer/Saito",
    "support": {
      "forum": "http://saito.siezi.com/forum/"
    }
  },
  "repositories": [
    {
      "type": "pear",
      "url": "http://pear.cakephp.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "Plugin\\": "Plugin/",
      "Saito\\": "Lib/Saito/"
    }
  },
  "config": {
    "vendor-dir": "Vendor/"
  },
  "require": {
    "php": ">=5.4.0",
    "ext-mcrypt": "*",
    "markstory/geshi": "0.3.*",
    "pear-pear.cakephp.org/CakePHP": "2.6.0-RC1",
    "kzykhys/ciconia": "~1.0.3",
    "jbbcode/jbbcode": "~1.3",
    "aura/di": "~2.1"
  },
  "require-dev": {
    "phpmd/phpmd": "@stable",
    "phpunit/phpunit": "3.7.*"
  }
}
